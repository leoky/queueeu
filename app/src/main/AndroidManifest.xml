<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leoky.queueeu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.SearchActivity" />
        <activity android:name=".Activity.SearchDetailActivity" />
        <activity android:name=".Activity.QueueDetailActivity" />
        <activity android:name=".Activity.OnProgress" />
        <activity android:name=".Activity.ChangeEmail" />
        <activity android:name=".Activity.ChangePassword" />
        <activity android:name=".Activity.ChangePhone" />
        <activity android:name=".Activity.History"></activity>
    </application>

</manifest>